<Application x:Class="SignalRNotif.MainUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SignalRNotif.MainUI"
             StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ImageBrush x:Key="SettingsLogoPrincipalBrush" ImageSource="Images/MLNotificationLogo.png" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MLDarkStyle.xaml" />
                <ResourceDictionary Source="Simple Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
